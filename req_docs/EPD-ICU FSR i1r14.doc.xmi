    <sectionContent xsi:type="micobs.doc:DDSection" name="General">
      <sectionContent xsi:type="micobs.doc:DDSection" name="Common software requirements">
        <sectionContent xsi:type="micobs.doc:DDBody">
          <bodyContent xsi:type="micobs.doctpl:DBasicTable" style="TableGridFirstRowColummShadow" width="100">
