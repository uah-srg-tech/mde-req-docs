  <section name="Introduction">
    <sectionContent xsi:type="micobs.doc:DDSection" name="Purpose">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The Software Requirements Specification defines and describes the requirements of the EPD ICU Software of the SRG Mission. The requirements described in this document are derived from "/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.1/@sectionContent.0/@bodyContent.0">
            <run italics="true">
              <text content="AD1"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
    <sectionContent xsi:type="micobs.doc:DDSection" name="Scope">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="This document applies to the EPD ICU Software of the SRG Mission. It does not provide information for the Ground Support Software (GSS), which is maintained separately as part of the Electronic Ground Support Equipment (EGSE). This document supplies information applicable to the Software Requirements Specification."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
  </section>
