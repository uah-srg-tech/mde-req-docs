  <section name="Introduction">
    <sectionContent xsi:type="micobs.doc:DDSection" name="Purpose">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The Software System Specification defines and describes the specification of the EPD ICU Software of the SRG Mission."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
    <sectionContent xsi:type="micobs.doc:DDSection" name="Scope">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="This document applies to the EPD ICU Software of the SRG Mission. It does not provide information for the Ground Support Software (GSS), which is maintained separately as part of the Electronic Ground Support Equipment (EGSE). This document supplies information applicable to the Software System Specification."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
  </section>
