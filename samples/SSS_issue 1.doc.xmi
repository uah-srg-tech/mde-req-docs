  <section name="Software Overview">
    <sectionContent xsi:type="micobs.doc:DDSection" name="Product Perspective">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The SRG Mission has been conceived to study the effect of high energy particles in "/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.8/@sectionContent.3/@bodyContent.0">
            <run italics="true">
              <text content="TBD Environment"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content=". The SRG Energetic Particle Detector (EPD) suite consists of four sensors measuring "/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.8/@sectionContent.4/@bodyContent.0">
            <run italics="true">
              <text content="TBD Particles"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content=". SRG EPD ICU Software purpose is to provide support to EPD sensors."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
    <sectionContent xsi:type="micobs.doc:DDSection" name="General Capabilities">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun" bold="true">
            <text content="The main capabilities of the SRG EPD ICU Software are the following:"/>
          </paragraphContent>
        </bodyContent>
        <bodyContent xsi:type="micobs.doctpl:DItemize">
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To collect scientific data from the instruments periodically."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To accumulate high-cadence science data from the sensors to specified lower time resolution packages."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To collect housekeeping data from the four units."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To time tag the data if necessary."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To perform lossless compression of the data if necessary."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To format the data for transmission by the S/C."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To accept and syntactically check TC from the S/C."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To deliver these commands to the individual sensors for execution."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To accept and syntactica"/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To deliver these commands to the individual sensors for execution."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To accept from the ground and distribute to the sensors tables and software."/>
              </paragraphContent>
            </paragraph>
          </items>
          <items>
            <paragraph>
              <paragraphContent xsi:type="micobs.doctpl:DRun">
                <text content="To handle the burst mode."/>
              </paragraphContent>
            </paragraph>
          </items>
        </bodyContent>
      </sectionContent>
    </sectionContent>
    <sectionContent xsi:type="micobs.doc:DDSection" name="General Constraints">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The read-only memory available for the storage of SRG EPD ICU Boot Software will be."/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.8/@sectionContent.6/@bodyContent.0">
            <run italics="true">
              <text content="TBD MBytes"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="."/>
          </paragraphContent>
        </bodyContent>
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The non-volatile memory available for the storage of SRG EPD ICU persistent data and Application Software will be ."/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.8/@sectionContent.5/@bodyContent.0">
            <run italics="true">
              <text content="TBD MBytes"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="."/>
          </paragraphContent>
        </bodyContent>
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="The memory available to SRG EPD ICU Software for sensor science storage will be limited to ."/>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.8/@sectionContent.7/@bodyContent.0">
            <run italics="true">
              <text content="TBD MBytes"/>
            </run>
          </paragraphContent>
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
    </sectionContent>
    <sectionContent xsi:type="micobs.doc:DDSection" name="Operational Environment">
      <sectionContent xsi:type="micobs.doc:DDBody">
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="Figure 1 illustrates the EPD Instrument Control Unit operational environment."/>
          </paragraphContent>
        </bodyContent>
        <bodyContent xsi:type="micobs.doctpl:DFigureFromFile" name="ICU IFs" caption="ICU Interfaces" referenceFile="SSS_images/icu_ifs.png" width="&quot;790&quot;" height="&quot;512&quot;"/>
        <bodyContent xsi:type="micobs.doctpl:DParagraph">
          <paragraphContent xsi:type="micobs.doctpl:DRun">
            <text content="EPD's ICU provides the interface between the SRG Spacecraft C&amp;amp;DH system and the EPD sensors. All information transfer between the EPD's sensors and the Spacecraft/Ground flows through the ICU, including telemetry, commands and status. The EPD's sensors communicate with the EPD's ICU over a dedicated serial interface."/>
          </paragraphContent>
        </bodyContent>
      </sectionContent>
      <sectionContent xsi:type="micobs.doc:DDSection" name="Sensors Interface">
        <sectionContent xsi:type="micobs.doc:DDBody">
          <bodyContent xsi:type="micobs.doctpl:DParagraph">
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content="EPD ICU Software will send/receive telecommand/telemetry to/from the sensors. The telemetry information includes both scientific and housekeeping data. The telecommand information includes sensors tables, sensor software and ground commands delivered from the S/C to the EPD ICU."/>
            </paragraphContent>
          </bodyContent>
        </sectionContent>
      </sectionContent>
      <sectionContent xsi:type="micobs.doc:DDSection" name="S/C Interface">
        <sectionContent xsi:type="micobs.doc:DDBody">
          <bodyContent xsi:type="micobs.doctpl:DParagraph">
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content="EPD ICU is a remote terminal on the spacecraft SpaceWire link. The implementation will be compliant up to the RD2 and RD3. Two separate SpaceWire links in a hot redundant configuration will be employed. SpaceWire is the command and data interface with the S/C."/>
            </paragraphContent>
          </bodyContent>
          <bodyContent xsi:type="micobs.doctpl:DParagraph">
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content="EPD command messages are contained in CCSDS telecommand packets. EPD CDPU strips off the CCSDS packet headers and analyses the destination identifier. If the identifier is associated to the ICU, the telecommand is processed directly by the ICU. If the identifier is associated to any sensor, the ICU forward the command to the specific sensor. Command responses from the sensor are sent to the ICU that transmits them to the S/C in CCSDS telemetry packets format. Command responses generated by the ICU itself are also CCSDS formatted and transmitted to the S/C."/>
            </paragraphContent>
          </bodyContent>
          <bodyContent xsi:type="micobs.doctpl:DParagraph">
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content="Transfer of data and command response from CDPU to the S/C shall take place according to the CCSDS recommendations defined in "/>
            </paragraphContent>
            <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.2/@sectionContent.0/@bodyContent.0">
              <run italics="true">
                <text content="RD1"/>
              </run>
            </paragraphContent>
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content=" and "/>
            </paragraphContent>
            <paragraphContent xsi:type="micobs.doctpl:DHyperlink" reference="//@section.2/@sectionContent.1/@bodyContent.0">
              <run italics="true">
                <text content="RD2"/>
              </run>
            </paragraphContent>
            <paragraphContent xsi:type="micobs.doctpl:DRun">
              <text content="."/>
            </paragraphContent>
          </bodyContent>
        </sectionContent>
      </sectionContent>
    </sectionContent>
