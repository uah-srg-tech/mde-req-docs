portConfig sis {
	uri := es.uah.aut.srg.sis;
	version := v1;
	uartPort {
		number := 2;
		baudRate := 115200bauds;
		parity := NO;
		dataBits := 8bits;
		stopBits := 1bit;
		inputQueueSize := 512;
		outputQueueSize := -1;
		uartProtocol {
			unit := bytes;
			power := no;
			constSize {
				bytes := 6;
				bits := 0;
			};
			sizeFieldOffset {
				bytes := 4;
				bits := 0;
			};
			sizeFieldTrim {
				bytes := 2;
				bits := 0;
			};
		};
	};
};