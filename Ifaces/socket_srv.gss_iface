portConfig socket_srv {
	uri := es.uah.aut.srg.socket_srv;
	version := v1;
	socketSrvPort {
		portNumber := 2000;
		portProtocol {
			unit := bytes;
			power := no;
			constSize {
				bytes := 6;
				bits := 0;
			};
			sizeFieldOffset {
				bytes := 4;
				bits := 0;
			};
			sizeFieldTrim {
				bytes := 2;
				bits := 0;
			};
			syncPattern {
				hexValue := 0xBEBABEEF;
			};
		};
	};
};